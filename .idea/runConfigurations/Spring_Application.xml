    <extension name="coverage" enabled="false" merge="false" sample_coverage="true" runner="idea">
      <pattern>
        <option name="PATTERN" value="io.engi.receipts.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method />
  </configuration>
</component>